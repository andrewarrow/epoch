go mod tidy
mkdir -p views
./epoch render
cp main.go save_main
cp wasm/main.go .
GOOS=js GOARCH=wasm go build -ldflags="-s -w -X main.useLive=truee" -o assets/other/json.wasm 
cd assets/other
rm json.wasm.gz
gzip json.wasm
cd ../..
mv save_main main.go
tailwindcss -i assets/css/tail.components.css -o assets/css/tail.min.css --minify
uuid=$(uuidgen); go build -ldflags="-X main.buildTag=$uuid" -o epoch
#cp epoch epoch.app/Contents/MacOS/MyApp
#open .
echo 3
./epoch
